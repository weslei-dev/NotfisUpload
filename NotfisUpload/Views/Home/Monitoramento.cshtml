@model IEnumerable<NotfisUpload.Entidade.Intercambio>


<br />
<div class="panel panel-default">
    <div class="panel-body">
        <form class="form" method="get">
            <div class="row">
                <div class="col-sm-5 col-xs-12">
                    <input type="text" class="form-control input-sm" placeholder="Pesquise por chaveNFE" id="ChaveNFE" name="ChaveNfe">
                </div>
                <div class="col-sm-3 col-xs-12">
                    <label class="sr-only">Nro. NF</label>
                    <input type="text" class="form-control input-sm" placeholder="Pesquise por Pedido" id="Pedido" name="Pedido">
                </div>
                <div class="col-sm-3 col-xs-12">
                    <input type="submit" class="btn btn-info btn-sm btn-block" value="Filtrar">
                </div>
            </div>
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th style="width: 30px;">#</th>
                        <th style="width: 30px;">#</th>
                        <th style="width: 200px;">Nome</th>
                        <th style="width: 250px;">CNPJ</th>
                        <th style="width: 100px;">Situação</th>
                        <th style="width: 150px;">Inclusão</th>
                        <th style="width: 30px;">Tipo</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@Html.DisplayFor(modelitem => item.Id)</td>
                            <td>@Html.DisplayFor(modelitem => item.CNPJ)</td>
                            <td>@Html.DisplayFor(modelitem => item.Data)</td>
                            <td>@Html.DisplayFor(modelitem => item.IE)</td>
                            <td>@Html.DisplayFor(modelitem => item.NotasFiscais)</td>
                            <td>@Html.DisplayFor(modelitem => item.Remetente)</td>
                            <td>@Html.DisplayFor(modelitem => item.Destinatario)</td>

                        </tr>
                    }
                </tbody>
            </table>

        </form>
    </div>
</div>
<!-- Grid -->
